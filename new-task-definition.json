{
    "family": "balekai-task",
    "taskRoleArn": "arn:aws:iam::422456985301:role/balekai-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::422456985301:role/balekai-ecs-exec-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "containerDefinitions": [
        {
            "name": "balekai",
            "image": "422456985301.dkr.ecr.us-east-1.amazonaws.com/balekai-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "FIREBASE_PROJECT_ID",
                    "value": "trelllo-7a26a"
                },
                {
                    "name": "SPRING_DATASOURCE_URL",
                    "value": "jdbc:postgresql://balekai-postgres-new.cy7yeimaywkm.us-east-1.rds.amazonaws.com:5432/balekai_db"
                },
                {
                    "name": "FIREBASE_CLIENT_EMAIL",
                    "value": "firebase-adminsdk-fbsvc@trelllo-7a26a.iam.gserviceaccount.com"
                },
                {
                    "name": "FIREBASE_PRIVATE_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:422456985301:secret:balekai-firebase-private-key-new-L9mgwB"
                },
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:422456985301:secret:balekai-jwt-secret-new-dLHNQ8"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "SPRING_DATASOURCE_USERNAME",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:422456985301:secret:balekai-db-credentials:username::"
                },
                {
                    "name": "SPRING_DATASOURCE_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:422456985301:secret:balekai-db-credentials:password::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/balekai/backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "volumes": [],
    "placementConstraints": []
}